  display: flex;
  width: 750rpx;
}
.row view {
  background-color: #4acdd6;
  width: 375rpx;
  text-align: center;
  color: rgb(196, 93, 93);
  font-size: 30rpx;
  padding: 15rpx 0;
}
.grade_year, .grade_line {
  background-color: rgb(211, 247, 247);
