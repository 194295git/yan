}
.admissionsline_option view,.admissionsline_option picker{
  display: inline-block;
  border-top: 1px solid #55c4b8;
  border-bottom: 1px solid #55c4b8;
  width: 150rpx;
  text-align: center;
  padding: 15rpx 0;
}
.admissionsline_option ._i {
  display: inline-block;
}
.college_grade {
  width: 750rpx;
  margin-top: 30rpx;
}
.row, .grade_row {
