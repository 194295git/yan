
#container {
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-pack: center;
  -webkit-justify-content: center;
          justify-content: center;
  -webkit-flex-wrap: wrap;
          flex-wrap: wrap;
  width: 750rpx;
  margin-bottom: 300rpx;
  background: rgb(225, 200, 250);
}
.province_line {
  width: 650rpx;
  margin-top: 50rpx;
}
.admissionsline_option {
  margin-top: 40rpx;
