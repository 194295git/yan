}
.form_line {
  width: 660rpx;
  margin-top: 50px;
  border-bottom: 1px solid #55c4b8;
  position: relative
}
.form_form{
  position: relative
}
.form_san {
  width: 0;
  height: 0;
  border:solid ;
  border-width: 5px 10px;
  border-left-color: #55c4b8;
  border-right: none;
  border-top-color: transparent;
  border-bottom-color: transparent;
  position: absolute;
  right: 0px;
  bottom: -5px;
}
#myCanvas{
  position: absolute;
  height: 154px;
  width:  660rpx;
}
.form_year{
  width: 660rpx;
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
  -webkit-box-align: center;
  -webkit-align-items: center;
          align-items: center;
  -webkit-box-pack: justify;
  -webkit-justify-content: space-between;
          justify-content: space-between;
