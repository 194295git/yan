  font-size: 30rpx;
  text-align: center;
}
.fa-thumb-tack{
  display: block;
  position: absolute;
  color: #55c4b8;
  font-size: 20px;
  margin-left: -3px;
  -webkit-transform: rotate(10deg);
          transform: rotate(10deg)
}
.form_grade{
  position: absolute;
  font-size: 25rpx;
  color: #6B6A6A;
}
.col_line{
