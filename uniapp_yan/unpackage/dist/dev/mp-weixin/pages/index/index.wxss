@-webkit-keyframes ciecle_enlarge {
0% {
    opacity: 0;
    -webkit-transform: scale(0.5);
            transform: scale(0.5);
}
70% {
    -webkit-transform: scale(1.3);
            transform: scale(1.3);
}
80% {
    -webkit-transform: scale(0.9);
            transform: scale(0.9);
}
90% {
    -webkit-transform: scale(1.2);
            transform: scale(1.2);
}
100% {
    opacity: 1;
    -webkit-transform: scale(1);
            transform: scale(1);
}
}
