/*志愿专业圆放大*/
@-webkit-keyframes close_rotate {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
    -webkit-transform:rotate(720deg);
            transform:rotate(720deg);
}
}
@keyframes close_rotate {
0% {
    opacity: 0;
}
100% {
    opacity: 1;
    -webkit-transform:rotate(720deg);
            transform:rotate(720deg);
}
}
/*交叉旋转*/
@-webkit-keyframes ciecle_narrow {
0% {
    opacity: 1;
    -webkit-transform: scale(1);
            transform: scale(1);
}
100% {
    opacity:0;
    -webkit-transform: scale(0.1);
            transform: scale(0.1);
}
}
